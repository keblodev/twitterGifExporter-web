{
  "private": false,
  "name": "twittergifexport-web",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --progress --mode=development",
    "start": "serve -p 3000 build -s -c 1",
    "prestart": "npm run build",
    "start_serve": "serve -p 3000 build -s -c 1",
    "start_report": "cross-env GEN_ANALITICS=run npm start",
    "build": "cross-env NODE_ENV=production webpack --progress --mode=production",
    "prebuild": "mkdirp build && ncp src/assets build/assets",
    "test": "jest cov",
    "test_e2e": "IS_BQ_E2E=true WITH_MONGO=true WITH_LOG_PROCESS=true WITH_PS_LIST=true BOOQED_MONGO_TEST_ALIAS=localhost jest e2e",
    "test_e2e_local": "npm run build && BOOQED_MONGO_TEST_ALIAS=localhost IS_BQ_E2E=true WITH_MONGO=true jest e2e && killall node && killall mongod",
    "test_cnt": "jest cnt",
    "test_unit": "jest unit",
    "test_cov": "jest cov --coverage",
    "test_unit_brk": "BOOQED_ENV=test node --inspect-brk node_modules/.bin/jest unit --runInBand --watch",
    "test_cnt_brk": "BOOQED_ENV=test node --inspect-brk node_modules/.bin/jest cnt --runInBand --watch",
    "test_e2e_brk": "BOOQED_ENV=test node --inspect-brk node_modules/.bin/jest e2e --runInBand --watch",
    "lint": "eslint src test",
    "setup_server": "cd server && npm i"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@testing-library/react": "^9.4.0",
    "acorn": "^6.2.0",
    "autoprefixer": "8.0.0",
    "babel-loader": "^8.0.6",
    "chai": "4.0.2",
    "clean-webpack-plugin": "0.1.19",
    "copy-webpack-plugin": "4.5.1",
    "core-js": "2.4.1",
    "cross-env": "5.0.1",
    "css-loader": "^3.4.2",
    "dotenv-webpack": "1.5.7",
    "ejs-compiled-loader": "1.1.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "json-loader": "0.5.4",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "less-vars-loader": "1.1.0",
    "mime": "2.3.1",
    "mini-css-extract-plugin": "0.4.5",
    "mkdirp": "0.5.1",
    "ncp": "2.0.0",
    "offline-plugin": "5.0.3",
    "postcss-loader": "^3.0.0",
    "progress-bar-webpack-plugin": "1.11.0",
    "raw-loader": "0.5.1",
    "regenerator-runtime": "0.10.3",
    "replace-bundle-webpack-plugin": "1.0.0",
    "robotstxt-webpack-plugin": "^6.0.0",
    "script-ext-html-webpack-plugin": "2.0.1",
    "sinon": "4.2.1",
    "sinon-chai": "2.9.0",
    "source-map-loader": "0.2.1",
    "style-loader": "0.20.0",
    "uglifyjs-webpack-plugin": "1.2.5",
    "url-loader": "0.6.1",
    "webpack": "4.29.3",
    "webpack-bundle-analyzer": "2.9.0",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "3.1.14"
  },
  "dependencies": {
    "node-polyglot": "^2.4.0",
    "promise-polyfill": "7.0.1",
    "prop-types": "^15.7.2",
    "proptypes": "1.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.0",
    "redux": "4.0.1",
    "redux-localstorage": "0.4.1",
    "redux-mock-store": "1.5.3",
    "redux-thunk": "2.3.0",
    "serve": "6.0.0"
  },
  "jest": {
    "setupFiles": [
      "./test/setup.js"
    ],
    "testURL": "http://localhost:8080",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ]
  }
}
