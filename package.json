{
  "private": true,
  "name": "twottergifexport-web",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --progress --mode=development",
    "start": "serve -p 5000 build -s -c 1",
    "start_serve": "serve -p $PORT build -s -c 1",
    "start_report": "cross-env GEN_ANALITICS=run npm start",
    "build": "cross-env NODE_ENV=production webpack --progress --mode=production",
    "prebuild": "mkdirp build && ncp src/assets build/assets",
    "test": "jest cov",
    "test_cnt": "jest cnt",
    "test_unit": "jest unit",
    "test_cov": "jest cov --coverage",
    "test_unit_brk": "ENV=test node --inspect-brk node_modules/.bin/jest unit --runInBand --watch",
    "test_cnt_brk": "ENV=test node --inspect-brk node_modules/.bin/jest cnt --runInBand --watch",
    "test_e2e_brk": "ENV=test node --inspect-brk node_modules/.bin/jest e2e --runInBand --watch",
    "lint": "eslint src test",
    "setup_server": "cd server && npm i"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {},
  "dependencies": {
    "@redux-beacon/google-analytics": "1.1.0",
    "history": "4.7.2",
    "preact": "8.3.1",
    "preact-compat": "3.18.4",
    "preact-redux": "2.0.3",
    "preact-render-to-string": "3.6.0",
    "preact-router": "2.6.1",
    "preact-router-redux": "4.1.0",
    "promise-polyfill": "7.0.1",
    "proptypes": "1.0.0",
    "query-string": "6.2.0",
    "react-code-splitting": "1.2.1",
    "react-form-with-constraints": "0.10.0",
    "redux": "4.0.1",
    "redux-beacon": "2.0.3",
    "redux-devtools": "3.4.1",
    "redux-devtools-dock-monitor": "1.1.3",
    "redux-devtools-log-monitor": "1.4.0",
    "redux-localstorage": "0.4.1",
    "redux-mock-store": "1.5.3",
    "redux-thunk": "2.3.0",
    "serve": "6.5.7",
    "url-pattern": "1.0.3",
    "autoprefixer": "8.0.0",
    "babel": "6.5.2",
    "babel-core": "6.24.0",
    "babel-eslint": "8.2.2",
    "babel-jest": "20.0.0",
    "babel-loader": "7.0.0",
    "babel-plugin-relative-import": "1.0.3",
    "babel-plugin-root-require": "1.3.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-react-jsx": "6.8.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.24.0",
    "babel-runtime": "6.11.6",
    "chai": "4.0.2",
    "clean-webpack-plugin": "0.1.19",
    "copy-webpack-plugin": "4.5.1",
    "core-js": "2.4.1",
    "cross-env": "5.0.1",
    "css-loader": "0.28.0",
    "dotenv-webpack": "1.5.7",
    "ejs-compiled-loader": "1.1.0",
    "eslint": "4.1.0",
    "eslint-plugin-jest": "21.7.1",
    "eslint-plugin-react": "7.0.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "20.0.0",
    "json-loader": "0.5.4",
    "less": "3.0.0",
    "less-loader": "4.0.3",
    "less-vars-loader": "1.1.0",
    "mime": "2.3.1",
    "mini-css-extract-plugin": "0.4.4",
    "mkdirp": "0.5.1",
    "ncp": "2.0.0",
    "offline-plugin": "5.0.3",
    "postcss-loader": "2.1.5",
    "preact-jsx-chai": "2.2.1",
    "preact-render-spy": "1.2.2",
    "progress-bar-webpack-plugin": "1.11.0",
    "raw-loader": "0.5.1",
    "regenerator-runtime": "0.10.3",
    "replace-bundle-webpack-plugin": "1.0.0",
    "script-ext-html-webpack-plugin": "2.0.1",
    "sinon": "4.2.1",
    "sinon-chai": "2.9.0",
    "source-map-loader": "0.2.1",
    "style-loader": "0.20.0",
    "uglifyjs-webpack-plugin": "1.2.5",
    "url-loader": "0.6.1",
    "webpack": "4.8.3",
    "webpack-bundle-analyzer": "2.9.0",
    "webpack-cli": "2.1.4",
    "webpack-dev-server": "3.1.4"
  },
  "jest": {
    "setupFiles": [
      "./test/setup.js"
    ],
    "testURL": "http://localhost:8080",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy",
      "^react$": "preact-compat",
      "^react-dom$": "preact-compat"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ]
  }
}
